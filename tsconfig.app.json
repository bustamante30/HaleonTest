{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "dev.env.ts",
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "tsconfig.cypress-ct.json",
    "src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "noImplicitAny": false,
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "allowJs": true,
    "moduleResolution": "node",
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "types": ["vue"],
    "skipLibCheck": true,
  }
}
