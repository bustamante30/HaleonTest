{
    "trailingSlash": "auto",
    "routes": [
      {
        "route": "/b2clogin",
        "redirect": "/b2clogin"
      },
      {
        "route": "/login",
        "redirect": "/login"
      }
    ],
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "responseOverrides": {
      "400": {
        "rewrite": "/invalid-invitation-error.html"
      },
      "401": {
        "redirect": "/login",
        "statusCode": 302
      },
      "403": {
        "rewrite": "/custom-forbidden-page.html"
      },
      "404": {
        "rewrite": "/404.html"
      }
    },
    "globalHeaders": {
      "content-security-policy": "default-src *; img-src * 'self' data: https: ; frame-src * 'self' data: blob: https: ; script-src 'self' 'unsafe-inline' 'unsafe-eval' *;style-src  'self' 'unsafe-inline' *"
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }